{
  "name": "Kabyle Gamers Premium",
  "version": "0.1",
  "description": "Ne ratez plus aucun événement de Kabyle Gamers !",
    "manifest_version": 2,
    "icons": {
      "128": "icon-def.png"
    },
    "browser_action":{
      "default_popup":"popup.html"
    },
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "tabs"
  ]
}
